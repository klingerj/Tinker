:set noundofile
:set backupdir=~/vimtmp//
:set directory=~/vimtmp//
:set guifont=Inconsolata_SemiBold:h12:W600:cANSI
:colorscheme briofita
:set ic
:set tabstop=4
:set expandtab
:set shiftwidth=4
:nnoremap <silent> <F1> :terminal<CR><c-w>x<c-w>k:vsplit<CR>
let @y = "build_app.bat Debug\n"
let @z = "build_app.bat Release\n"
nnoremap <m-m> <c-w>j
tnoremap <m-m> <c-w>"y<c-w>k
tnoremap <m-n> <c-w>"z<c-w>k
